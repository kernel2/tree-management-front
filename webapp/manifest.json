{
  "_version": "1.12.0",
  "sap.app": {
    "id": "sap.ui.demo.treemanagement",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "Tree Management App",
    "description": "A simple SAPUI5 app for managing tree data"
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "asyncHints": {
      "preloadBundles": []
    },
    "rootView": {
      "viewName": "sap.ui.demo.treemanagement.view.Main",
      "type": "XML",
      "async": true
    },
    "models": {
      "treesModel": {
        "type": "sap.ui.model.json.JSONModel",
        "settings": {}
      }
    }
  }
}
